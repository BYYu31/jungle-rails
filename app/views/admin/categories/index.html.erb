<section class="admin-catogoires-new">

  <header class="page-header">
  <h1>Admin &raquo; New Categories</h1>
  </header>

  <div class="well">
    <%= link_to '+ New Categories', [:new, :admin, :category], class: 'btn btn-info' %>
  </div>

  <div class="panel panel-default categories">

  <table class="table table-bordered">
    <thead>
      <tr>
        <th>ID</th>
        <th>Category</th>
      </tr>
    </thead>
    <tbody>
      <%= render @categories %>
    </tbody>
  </table>

</div>

</section>